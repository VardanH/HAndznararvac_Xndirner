function if_Permutation(str1, str2) { // return true if str1 Permutation of str2
var arr1 = str1.split("");
var arr2 = str2.split("");
arr1.sort();              
arr2.sort();  
var arr1_str=arr1.join("");                     
var arr2_str=arr2.join("");
if (arr1_str==arr2_str) return true;//console.log

return false;//console.log


}
